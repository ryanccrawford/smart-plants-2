{
  "name": "smart-plants-2",
  "version": "1.1.0",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "cd client && npm run build"
  },
  "author": "Ryan Crawford",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.1",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^0.31.1",
    "@google-cloud/vision": "^1.2.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "http-errors": "~1.6.3",
    "if-env": "^1.0.4",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.5",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "mysql2": "^1.6.5",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "path": "^0.12.7",
    "request": "^2.88.0",
    "sequelize": "^5.16.0",
    "validator": "^11.1.0",
    "whatwg-fetch": "^3.0.0"
  }
}
